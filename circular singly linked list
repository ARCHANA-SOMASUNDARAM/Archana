import java.util.*;
class Node {
    int data;
    Node next;
     Node(int data){
      this.data=data;
        next = null;
    }
}
class Circularsinglylinkedlist{
	Node head=null;
	Node tail=null;
	int size=0;
      void addatlast(int data){
        Node newNode = new Node(data);
        size++;
        if (head == null) {
            head = tail =newNode;
            tail.next =head;
        }
        else {
            tail.next = newnode;
            tail =newnode;
            tail.next=head;
            }
      }

   void addatbeg(int data){
        Node newnode =new node(data);
        size++;
        if(head==nul){
          head =tail=newnode;
          tail.neaxt=head;
        }
        newnode.next =head;
        head =newnode;
        tail.next=head;
      }
       void display(){
         if(head==null){
           System.out.println("there is no element to print");
           return;
         }
         Node temp =head;
         do{
           System.out.print(temp.data+" ");
           temp=temp.next;
         }while(temp!=head);
         System.out.println();
         }
       }
       public class main{
         public static void main(String[]args){
           Circularsinglylinkedlist c=new Circularsinglylinkedlist();
           c.addatlast(10);
           c.addatlast(20);
           c.addatlast(30);
           c.addatlast(40);
         }
       }
        
        
      
